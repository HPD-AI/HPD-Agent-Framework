{
  "servers": [
    {
      "name": "context7",
      "command": "npx",
      "arguments": ["-y", "@upstash/context7-mcp"],
      "description": "Up-to-date library documentation and code examples from Context7",
      "enabled": false,
      "enableCollapsing": true,
      "requiresPermission": false,
      "timeoutMs": 30000,
      "retryAttempts": 3,
      "environment": {
        "NODE_ENV": "production"
      }
    },
    {
      "name": "filesystem",
      "command": "npx",
      "arguments": ["-y", "@anthropic/mcp-filesystem", "."],
      "description": "File system operations for reading and writing files",
      "enabled": false,
      "enableCollapsing": true,
      "requiresPermission": true,
      "timeoutMs": 30000,
      "retryAttempts": 3
    },
    {
      "name": "fetch",
      "command": "npx",
      "arguments": ["-y", "@anthropic/mcp-fetch"],
      "description": "Fetch web content and URLs",
      "enabled": false,
      "enableCollapsing": true,
      "requiresPermission": true,
      "timeoutMs": 30000,
      "retryAttempts": 3
    }
  ]
}
